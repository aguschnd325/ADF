{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDFH"
		},
		"AzurePostgreSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePostgreSql1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"adfaas_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adfaas'"
		},
		"sql_asm_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_asm'"
		},
		"transformedtableSALES_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'transformedtableSALES'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzurePostgreSql1_connectionString": {
						"value": "[parameters('AzurePostgreSql1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"adfaas_connectionString": {
						"value": "[parameters('adfaas_connectionString')]"
					},
					"sql_asm_connectionString": {
						"value": "[parameters('sql_asm_connectionString')]"
					},
					"transformedtableSALES_connectionString": {
						"value": "[parameters('transformedtableSALES_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}