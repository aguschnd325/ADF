						"type": "AzurePostgreSqlSource",
						"query": "SELECT mt.rs_id as custno, cast(mt.barang_id as varchar) as item_no,cast(mb.nama as varchar) as item_desc,cast(mt.qty as decimal) as item_qty,cast(mt.updated_at as date) as sold_date,mrs.kode_rs as cust_name\nFROM public.m_transaction mt\nleft outer join public.m_rs mrs on mt.rs_id=mrs.id\nleft outer join public.m_barang mb on mt.barang_id=mb.id"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
