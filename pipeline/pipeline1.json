									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "item_desc",
									"type": "String",
									"physicalType": "character varying"
								},
								"sink": {
									"name": "item_desc",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
